{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.json"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    },
    {
      "name": "olb-covers",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "https://covers.openlibrary.org/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "nyt-api",
      "urls": [
        "/lists",
        "/lists/*",
        "https://api.nytimes.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "5s"
      }
    },
    {
      "name": "olb-api",
      "urls": [
        "/lists/*/*",
        "https://openlibrary.org/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "5s"
      }
    }
  ]
}
